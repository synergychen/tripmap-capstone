<%= render "error_messages", target: trip %>

<%= form_for trip do |form| %>
  <%= form.label :starts_on %>
  <%= form.datetime_local_field :starts_on %>

  <%= form.label :city %>
  <%= form.text_field :city %>

  <%= form.label :completed %>
  <%= form.check_box :completed %>

  <%= form.submit %>
<% end %>
