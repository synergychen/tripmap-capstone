<div class="all-trips main-box">
  <h3>View all trips</h3>

  <table>
    <tr>
      <th>Date</th>
      <th>Starts</th>
      <th>Estimated Ends</th>
      <th>Place</th>
      <th>Status</th>
    </tr>
    <%= render @trips %>
  </table>
</div>

<div class="new-trip-link main-box">
  <%= link_to "Create a new trip", new_trip_path %>
</div>
